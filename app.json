{
  "expo": {
    "name": "ActiveAxis",
    "slug": "ActiveAxis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/activeaxislogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/activeaxislogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ActiveAxis"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/activeaxislogo.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.ActiveAxis"
    },
    "web": {
      "favicon": "./assets/activeaxislogo.png"
    },
    "plugins": [
      "react-native-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 26
          }
        }
      ],
      "react-native-health",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter Medium.ttf",
            "./assets/fonts/Inter.ttf",
            "./assets/fonts/Inter SemiBold.ttf",
            "./assets/fonts/Poppins Medium.ttf",
            "./assets/fonts/Poppins SemiBold.ttf",
            "./assets/fonts/Fuzzy Bubbles.ttf"
          ]
        }
      ],
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": ["peripheral", "central"],
          "bluetoothAlwaysPermission": "Allow $(PRODUCT_NAME) to connect to bluetooth devices"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b49333ec-6e83-4739-ac15-06f5bfb9e045"
      }
    }
  }
}
