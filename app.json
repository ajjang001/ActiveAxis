{
  "expo": {
    "name": "ActiveAxis",
    "slug": "ActiveAxis",
    "owner": "activeaxisdevs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ActiveAxisLoading.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ActiveAxisLoading.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ActiveAxis"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ActiveAxisLoading.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.ActiveAxis",
      "permissions": [
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED",
        "android.permission.health.WRITE_ACTIVE_CALORIES_BURNED",
        "android.permission.health.READ_DISTANCE",
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_HEART_RATE",
        "android.permission.health.READ_TOTAL_CALORIES_BURNED",
        "android.permission.health.WRITE_TOTAL_CALORIES_BURNED",
        "android.permission.health.READ_SPEED"
      ]
    },
    "web": {
      "favicon": "./assets/ActiveAxisLoading.png"
    },
    "plugins": [
      "react-native-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 26
          }
        }
      ],
      "react-native-health",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/Poppins-Medium.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf",
            "./assets/fonts/Fuzzy-Bubbles.ttf",
            "./assets/fonts/League-Spartan-Light.ttf",
            "./assets/fonts/League-Spartan.ttf",
            "./assets/fonts/League-Spartan-SemiBold.ttf"
          ]
        }
      ],
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": [
            "peripheral",
            "central"
          ],
          "bluetoothAlwaysPermission": "Allow $(PRODUCT_NAME) to connect to bluetooth devices"
        }
      ],
      "expo-health-connect"
    ],
    "extra": {
      "eas": {
        "projectId": "b49333ec-6e83-4739-ac15-06f5bfb9e045"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5660392791887704~1776309303"
  }
}
